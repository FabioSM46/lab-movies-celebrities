<h2>Details</h2>
<p>{{movieID.title}}</p>
<p>{{movieID.genre}}</p>
<p>{{movieID.plot}}</p>

{{#each movieID.cast}}
{{#if this._id}}
<li>{{this.name}}</li>
<li>{{this.occupation}}</li>
<li>{{this.catchPhrase}}</li>
{{/if}}
{{/each}}
<form method="post" action="/movies/{{movieID.id}}/delete">
    <button type="submit">Delete</button>
    <a href="/movies/{{movieID.id}}/edit">Edit</a>
</form>